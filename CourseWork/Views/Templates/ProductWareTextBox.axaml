<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:CourseWork.ViewModels"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="vm:ProductWindowViewModel"
             x:Class="CourseWork.Views.Templates.ProductWareTextBox">
    <Grid Margin="0, 16, 0, 16">
        <TextBox CornerRadius="8" Watermark="Temperature" Text="{Binding ExpirationDate}" />
    </Grid>
</UserControl>